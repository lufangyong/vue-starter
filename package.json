{
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "@vueuse/integrations": "^9.10.0",
    "axios": "^1.2.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.29",
    "qs": "^6.11.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify-json/carbon": "^1.1.11",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.13",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/parser": "^5.20.0",
    "@unocss/reset": "^0.47.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue-macros/volar": "^0.5.4",
    "@vue/test-utils": "^2.2.6",
    "eslint": "^8.29.0",
    "husky": "^7.0.4",
    "jsdom": "^20.0.3",
    "less": "^4.1.3",
    "lint-staged": "12.3.7",
    "pnpm": "^7.18.1",
    "postcss-less": "^6.0.0",
    "taze": "^0.8.4",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-macros": "^1.0.3",
    "vite": "^4.0.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.27.1",
    "vitest": "^0.25.7",
    "vue-tsc": "^1.0.13"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ]
  }
}
